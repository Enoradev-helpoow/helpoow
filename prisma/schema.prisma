generator client {
  provider        = "prisma-client-js"
  output          = "../generated/client"
  previewFeatures = ["postgresqlExtensions"]
  binaryTargets   = ["native"]
}

datasource db {
  provider   = "postgresql"
  url        = env("DATABASE_URL")
}

model Annonce {
  id           Int      @id @default(autoincrement())
  titre        String
  description  String
  date         DateTime
  duree        String
  utilisateur  String
  image        String
  prix         Int
  localisation String
  categorie    String
  createdAt    DateTime @default(now())
}